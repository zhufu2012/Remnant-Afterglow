	%% 1代表不可加入队伍不显示组队列表
	%% 0代表可以加入队伍显示组队列表
	invitation,
	%% AutoBVT:
	%% 击杀其他非红名玩家是否获得红名值
	%% 0为不获得
	%% 1为获得
	redname,
	%% 作者:
	%%  是否保留尸体
	%% 0不保留
	%% 1保留
	corpseRemain,
	%% 作者:
	%% 是否直线寻路
	%% 0不是直线1是直线
	autoMoveStraight,
	%% 服务器副本用
	%% 0刷怪读地图文件
	%% 1刷怪读表
	excelMonster,
	%% 场景资源
	mapRes,
	%% 作者:
	%% 客户端的地图的配置文件
	%% trunk\Client\Assets\StreamingAssets\MapInfo\
	mapInfo_Loc,
	%% 作者:
	%% 服务器的地图的配置文件
	%% trunk\Client\Assets\OutPut\ForServer\MapInfo
	mapInfo_Sev,
	%% 作者:
	%% 客户端用的路点
	wayPoint,
	%% 作者:
	%% 地图出生点
	%% trunk\Client\Assets\StreamingAssets\PathGrid\
	pathGrid,
	%% 作者:
	%% 地图引用的剧情动画文件
	cinematicBin,
	%% 进入副本触发的Cinematic
	enterCinematicName,
	%% 胜利结算条件触发时延迟结算的时间（服务器副本适用）（秒）
	successWait,
	%% 进入副本动画播放的类型
	%% 0只有在接有某任务的情况下触发
	%% 1每次进入都播放（低端机的主城动画不播放）
	%% 2每次进入都播放（所有机的主城动画播放）
	%% 举例：0|1001为只有在接受任务1001的情况下才触发剧情。1|0总是触发
	enterCinematicPlay,
	%% 作者:
	%% 进入副本的镜头移动。用剧情动画实现。
	enterCameraMove,
	%% 作者:
	%% 摄像机Y轴偏移
	%% 数字越大越往上
	cameraTargetOffy,
	%% 作者:
	%% 摄像机透视
	cameraFov,
	%% 作者:
	%% 镜头距离。
	%% -1，地图默认读取玩家设置的距离
	%% 其他值，读取配置值
	cameraDistance,
	%% 摄像机高度
	cameraPitch,
	%% 摄像机旋转角度
	cameraYaw,
	%% 未的副本在没有人的时候保留的时间（秒）
	mapRetain,
