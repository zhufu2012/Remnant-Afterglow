	%% 这个副本的boss受击时表现掉落什么东西
	%% 0不掉
	%% 1掉金币
	%% 2掉饺子
	%% 3掉饺子和包子
	%% 4掉包子
	%% 5掉装备强化石
	%% 678留着以后扩展
	bossHitDrop,
	%% 地图获得怒气修正
	%% 受伤回怒，技能回怒修正
	%% 怒气*RageCor
	rageCor,
	%% 作者:
	%% boss说话id
	monsterTalk,
	%% 作者:
	%% 小地图资源路径|UI上x坐标偏移|UI上y坐标偏移|缩放比例
	miniMap,
	%% 作者:
	%% 地图安全区。
	%% 配置方式：{中心X坐标，中心z坐标，矩形x轴size的一半，矩形z轴size的一半}|{中心X坐标，中心z坐标，矩形x轴size的一半，矩形z轴size的一半}。
	%% 没有安全区填0.
	safe,
	%% 是否推荐守护
	%% 0不推荐
	%% 1推荐经验守护
	%% 2.推荐经验守护
	guard,
	%% 守护快捷拾取是够生效
	%% 0不生效
	%% 1生效
	quickpickup,
	%% 经验药水使用
	%% （经验药水类型1，经验药水ID）
	%% 经验药水类型：序号
	%% 经验药水ID：道具ID
	useExpWater,
	%% 徽记使用
	%% （徽记类型1，徽记ID）
	%% 徽记类型：序号
	%% 徽记ID：道具ID
	%% 是否能快捷购买
	useEmblem,
	%% 鼓舞(鼓舞类型1,消耗类型,具体消耗,单次消耗数量,可鼓舞次数)|(鼓舞类型2,消耗类型,具体消耗,单次消耗数量,可鼓舞次数)
	%% 鼓舞类型：序号，对应鼓舞的位置和后面的鼓舞buff
	%% 消耗类型1：道具，具体消耗：道具ID
	%% 消耗类型2：货币，具体消耗：货币ID
	%% 单次消耗数量：道具/货币的数量
	%% 可鼓舞次数：鼓舞次数
	inspireConsume,
	%% （鼓舞类型1增加的buff）|（鼓舞类型2增加的buff）
	inspireAttribute,
	%% 进入服务器副本变身。(变身ID,时间)
	%% 时间为-1时本地图持续变身
	%% 整体填0不变身
	%% 时间填0不变身
	transformID,
	%% 进入服务器副本后，额外获得一定数量的怒气
	addAnger,
	%% 在该服务器副本中释放XP变身技能，变身时间将会得到增加。填写-1，变身时间不会减少，在地图一直持续，填其他正数增加XP技持续时间，毫秒
	xPLastTime,
	%% 队伍复活ID.调用复活表Revive_1的ID，同时存在3种复活方式.填0不复活
	reviveID,
	%% 角色复活ID.调用复活表Revive_2的ID，同时存在3种复活方式.填0不复活
	reviveAct,
	%% 复活点。参数1为阵营，参数2-4为XZR
	revivePoint,
	%% 阶段目标
	%% 为表格MapStage的ID，可以多个
	mapStage,
	%% 系统功能ID
	%% 功能与地图对应
	mapOpenaction
}).

