	%% 进入最小等级限制
	%% 0为不限制
	level_min,
	%% 作者:
	%% 进入最大等级限制
	%% 0为不限制
	level_max,
	%% 作者:
	%% 地图最大进入人数
	player_max,
	%% 秒（单位）。单人副本中，这个限时代表BOSS限时，服务器副本中，这个限时代表副本总限时。
