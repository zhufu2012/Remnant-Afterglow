	%% 0不启用
	%% 1启用
	%% 启用之后Pkmode字段不生效
	force,
	%% AutoBVT:
	%% {2,1}|{1,1}|{3,0}
	%% 默认第一个为默认模式，配置的模式客户端全部显示
	%% {模式编号，是否可选}0为不可选，1为可选
	%% 1.和平模式
	%% 2.友方模式
	%% 3.全体模式
	%% 4.跨服模式
	%% 5.入侵模式
	%% 6.防守模式
	pKmode,
